<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>exe</OutputType>
	<DocumentationFile>bin\Release\netcoreapp3.1\rvm.xml</DocumentationFile>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyVersion>0.9.0.0</AssemblyVersion>
    <ApplicationIcon>robin.ico</ApplicationIcon>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>robin.png</PackageIcon>
    <Description>RobinVirtualMachine library</Description>
    <Product>RobinVirtualMachine</Product>
    <Company>.</Company>
    <Authors>Carpal</Authors>
    <Version>0.9</Version>
    <Copyright />
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/Carpall/rvm/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Carpall/rvm/</RepositoryUrl>
    <RepositoryType>GitHub OpenSource Public</RepositoryType>
    <PackageTags>virtuamachine vm robin</PackageTags>
    <PackageReleaseNotes>First release</PackageReleaseNotes>
    <NeutralLanguage />
  </PropertyGroup>

  <ItemGroup>
    <None Include="robin.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="src\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
  </Target>

</Project>
